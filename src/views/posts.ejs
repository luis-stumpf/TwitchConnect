<%- include("partials/header") -%> 

<h1>Feed</h1>

<% posts.slice().reverse().forEach(function(post){ %>
    <div class="post-container">
        <h2><%=post.title%></h2>
        <p>
        <%=post.content%>
        <br>
        
        <div class="writer">
            <%= post.time %>
            <a href="https://www.twitch.tv/<%= post.writer %>" target="_blank" class="btn btn-success"><img src="<%= post.writerImg %> " alt="profile-img" class="profile-img"> <%= post.writer %></a>
        </div>
        </p>  
    </div>
<% }) %>

<%- include("partials/footer"); -%>